<Window x:Class="CalculadoraCientifica.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculadoraCientifica"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Numero 1"/>
            <TextBox Width="100" Height="30" Text="{Binding Path=Numero1, Mode=TwoWay}"/>
            <Label Content="Numero 1"/>
            <TextBox Width="100" Height="30" Text="{Binding Path=Numero2, Mode=TwoWay}"/>
            <Label Content="Resultado"/>
            <TextBox Width="100" Height="30" Text="{Binding Path=Resultado, Mode=TwoWay}"/>




        </StackPanel>


    </Grid>
</Window>
